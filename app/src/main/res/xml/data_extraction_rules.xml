<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!--
        All androidx.datastore.preferences files are backed up by default.
        Use <exclude> to prevent specific files from being backed up.
        <exclude domain="sharedpref" path="my_sensitive_data.xml"/>
        -->
    </cloud-backup>
    <!--
    <device-transfer>
        <include .../>
        <exclude .../>
    </device-transfer>
    -->
</data-extraction-rules> 